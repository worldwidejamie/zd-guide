{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wwj-zdguide/article",
  "version": "0.1.0",
  "title": "Zendesk Article",
  "category": "widgets",
  "icon": "book-alt",
  "description": "Display a Zendesk Guide article.",
  "example": {},
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "wwj-zdguide",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "attributes": {
    "articleId": {
      "type": "number",
      "default": 0
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showMeta": {
      "type": "boolean",
      "default": true
    },
    "align": {
      "type": "string"
    }
  }
}
